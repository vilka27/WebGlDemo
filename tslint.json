{
  "defaultSeverity": "warn",
  "jsRules": {},
  "rules": {
    "semicolon": [true, "always"],
    "indent": [
      true,
      "spaces",
      4
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "no-var-requires": false,
    "no-console": false,
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "callable-types": true,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "eofline": false,
    "object-literal-sort-keys": false,
    "whitespace":[true,"check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"],
    "radix": true,
    "no-parameter-reassignment": true,
    "align":true,
    "prefer-readonly":true,
    "prefer-const": true,
    "one-line":true
  },
  "rulesDirectory": []
}